{
  "title": "AcceDe Web - tablist",
  "name": "accedeweb-tablist",
  "version": "1.0.2",
  "description": "WAI-ARIA tablist plugin based on AcceDe Web accessibility guidelines",
  "main": "dist/tablist.js",
  "module": "lib/index.js",
  "scripts": {
    "test": "tape test/*.js | faucet",
    "build": "rollup -c && rollup -c rollup.config.minified.js",
    "doc": "documentation build lib/*.js",
    "doc-lint": "documentation lint",
    "doc-serve": "documentation serve",
    "version": "npm run test && npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AcceDe-Web/tablist.git"
  },
  "keywords": [
    "accessibility",
    "tablist",
    "tabs",
    "accordion",
    "javascript",
    "vanilla",
    "a11y"
  ],
  "author": {
    "name": "Switch",
    "email": "bonjour@switch.paris",
    "url": "http://switch.paris"
  },
  "license": "ISC",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "homepage": "http://a11y.switch.paris/",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "documentation": "^6.2.0",
    "eslint": "^4.19.1",
    "eslint-config-idiomatic": "^4.0.0",
    "faucet": "0.0.1",
    "nightmare": "^2.3.3",
    "puppeteer": "^1.3.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0",
    "tape": "^4.9.0"
  }
}
