    "email": "bonjour@switch.paris",
    "url": "http://switch.paris"
  },
  "license": "ISC",
  "homepage": "http://a11y.switch.paris/",
  "devDependencies": {
    "documentation": "^4.0.0-beta2",
    "faucet": "0.0.1",
    "nightmare": "^2.3.3",
    "tape": "^4.5.1",
    "uglify-js": "^2.7.3"
  }
}
